{
  "codeFileHandlers": [
    { "type": "ForgedOnce.TsLanguageServices.CodeMixer.CSharp.CSharpCodeFileHandlerFactory, ForgedOnce.TsLanguageServices.CodeMixer.CSharp" },
    { "type": "ForgedOnce.TsLanguageServices.CodeMixer.LimitedTypeScript.LtsCodeFileHandlerFactory, ForgedOnce.TsLanguageServices.CodeMixer.LimitedTypeScript" }
  ],
  "inputs": {
    "DefinitionTree": "CSharp:Project:ForgedOnce.TsLanguageServices.LTS.Model/DefinitionTree/*.cs",
    "TypeData": "CSharp:Project:ForgedOnce.TsLanguageServices.LTS.Model/TypeData/*.cs",
    "ResultModel": "CSharp:Project:ForgedOnce.TsLanguageServices.LTS.Model/ResultModel/*.cs",
    "Common": "CSharp:Project:ForgedOnce.TsLanguageServices.LTS.Model/CodeFile.cs;ForgedOnce.TsLanguageServices.LTS.Model/CodeGenerationTask.cs"
  },
  "batches": [
    {
      "name": "Batch01",
      "stages": [
        {
          "name": "TypeScriptModelInterfaces",
          "plugin": {
            "pluginFactory": {
              "type": "ForgedOnce.TsLanguageServices.LTS.ModelTsInterfacesPlugin.PluginFactory, ForgedOnce.TsLanguageServices.LTS.ModelTsInterfacesPlugin",
              "config": {
                "modelBaseNamespace": "ForgedOnce.TsLanguageServices.LTS.Model",
                "outputFileName": "IntermadiateModel.ts"
              }
            }
          },
          "input": {
            "settings": {
              "DefinitionTree": "*.cs",
              "TypeData": "*.cs",
              "ResultModel": "*.cs",
              "Common": "*.cs"
            }
          },
          "output": {
            "settings": {
              "PassThrough": "*"
            }
          },
          "destinationMapping": {
            "PassThrough": {
              "name": "fileSystem",
              "settings": {
                "path": "..\\ForgedOnce.TsLanguageServices.CSToTS.TypeScriptBuilder"
              }
            }
          }
        }
      ]
    }
  ]
}
