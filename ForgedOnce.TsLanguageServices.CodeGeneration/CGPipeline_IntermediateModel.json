{
  "codeFileHandlers": [
    { "type": "ForgedOnce.CSharp.CSharpCodeFileHandlerFactory, ForgedOnce.CSharp" },
    { "type": "ForgedOnce.TypeScript.TsCodeFileHandlerFactory, ForgedOnce.TypeScript" }
  ],
  "inputs": {
    "DefinitionTree": "CSharp:Project:ForgedOnce.TsLanguageServices.Model/DefinitionTree/*.cs",
    "TypeData": "CSharp:Project:ForgedOnce.TsLanguageServices.Model/TypeData/*.cs",
    "ResultModel": "CSharp:Project:ForgedOnce.TsLanguageServices.Model/ResultModel/*.cs",
    "Common": "CSharp:Project:ForgedOnce.TsLanguageServices.Model/CodeFile.cs;ForgedOnce.TsLanguageServices.Model/CodeGenerationTask.cs"
  },
  "batches": [
    {
      "name": "Batch01",
      "stages": [
        {
          "name": "TypeScriptModelInterfaces",
          "plugin": {
            "pluginFactory": {
              "type": "ForgedOnce.TsLanguageServices.ModelTsInterfacesPlugin.PluginFactory, ForgedOnce.TsLanguageServices.ModelTsInterfacesPlugin",
              "config": {
                "modelBaseNamespace": "ForgedOnce.TsLanguageServices.Model",
                "outputFileName": "IntermediateModel.ts"
              }
            }
          },
          "input": {
            "settings": {
              "DefinitionTree": "*.cs",
              "TypeData": "*.cs",
              "ResultModel": "*.cs",
              "Common": "*.cs"
            }
          },
          "output": {
            "settings": {
              "PassThrough": "*"
            }
          },
          "destinationMapping": {
            "PassThrough": {
              "name": "fileSystem",
              "settings": {
                "path": "..\\ForgedOnce.TsLanguageServices.TSBuilder"
              }
            }
          }
        }
      ]
    }
  ]
}
