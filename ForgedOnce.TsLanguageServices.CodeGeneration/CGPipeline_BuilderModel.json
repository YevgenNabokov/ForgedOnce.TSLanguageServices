{
  "codeFileHandlers": [
    { "type": "ForgedOnce.CSharp.CSharpCodeFileHandlerFactory, ForgedOnce.CSharp" }
  ],
  "inputs": {
    "SourceTree": "CSharp:Project:ForgedOnce.TsLanguageServices.Model/DefinitionTree/*.cs"
  },
  "batches": [
    {
      "name": "Batch01",
      "stages": [
        {
          "name": "BuilderDefinitionTree",
          "plugin": {
            "pluginFactory": {
              "type": "ForgedOnce.TsLanguageServices.BuilderDefinitionTreePlugin.PluginFactory, ForgedOnce.TsLanguageServices.BuilderDefinitionTreePlugin",
              "config": {
                "outputNamespace": "ForgedOnce.TsLanguageServices.ModelBuilder.DefinitionTree",
                "sourceNodeBaseType": "ForgedOnce.TsLanguageServices.Model.DefinitionTree.Node",
                "trackingCollectionType": "ForgedOnce.TsLanguageServices.ModelBuilder.DefinitionTree.BuilderNodeCollection<>",
                "destinationNodeBaseType": "ForgedOnce.TsLanguageServices.ModelBuilder.DefinitionTree.Node",
                "typesToSkip": "ForgedOnce.TsLanguageServices.Model.DefinitionTree.Node",
                "allowedModificationCheckMethodName": "EnsureIsEditable"
              }
            }
          },
          "input": {
            "settings": {
              "SourceTree": "*.cs"
            }
          },
          "output": {
            "settings": {
              "PassThrough": "*"
            }
          },
          "destinationMapping": {
            "PassThrough": {
              "settings": { "path": "ForgedOnce.TsLanguageServices.ModelBuilder/DefinitionTree/Generated" }
            }
          },
          "cleanDestinations": true
        }
      ]
    }
  ]
}
