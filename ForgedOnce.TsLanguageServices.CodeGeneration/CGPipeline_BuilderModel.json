{
  "codeFileHandlers": [
    { "type": "ForgedOnce.TsLanguageServices.CodeMixer.CSharp.CSharpCodeFileHandlerFactory, ForgedOnce.TsLanguageServices.CodeMixer.CSharp" }
  ],
  "inputs": {
    "SourceTree": "CSharp:Project:ForgedOnce.TsLanguageServices.LTS.Model/DefinitionTree/*.cs"
  },
  "batches": [
    {
      "name": "Batch01",
      "stages": [
        {
          "name": "BuilderDefinitionTree",
          "plugin": {
            "pluginFactory": {
              "type": "ForgedOnce.TsLanguageServices.LTS.BuilderDefinitionTreePlugin.PluginFactory, ForgedOnce.TsLanguageServices.LTS.BuilderDefinitionTreePlugin",
              "config": {
                "outputNamespace": "ForgedOnce.TsLanguageServices.LTS.Builder.DefinitionTree",
                "sourceNodeBaseType": "ForgedOnce.TsLanguageServices.LTS.Model.DefinitionTree.Node",
                "trackingCollectionType": "ForgedOnce.TsLanguageServices.LTS.Builder.DefinitionTree.BuilderNodeCollection<>",
                "destinationNodeBaseType": "ForgedOnce.TsLanguageServices.LTS.Builder.DefinitionTree.Node",
                "typesToSkip": "ForgedOnce.TsLanguageServices.LTS.Model.DefinitionTree.Node",
                "allowedModificationCheckMethodName": "EnsureIsEditable"
              }
            }
          },
          "input": {
            "settings": {
              "SourceTree": "*.cs"
            }
          },
          "output": {
            "settings": {
              "PassThrough": "*"
            }
          },
          "destinationMapping": {
            "PassThrough": {
              "settings": { "path": "ForgedOnce.TsLanguageServices.LTS.Builder/DefinitionTree/Generated" }
            }
          },
          "cleanDestinations": true
        }
      ]
    }
  ]
}
